<service>
	<id>zenterbridge.exe</id>
	<name>Zenter Bridge</name>
	<description>Zenter Bridge - Hikvision to Supabase background agent</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\zenter-bridge\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\zenter-bridge\bridge\hik_stream_to_supabase.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>Zenter Bridge wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<env name="NODE_ENV" value="production"/>
	<serviceaccount>
		<domain>DESKTOP-QEAQKIV</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\zenter-bridge</workingdirectory>
</service>